<Window x:Class="MusGen.View.Windows.LoggerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusGen.View.Windows"
        mc:Ignorable="d"
        Title="Logs" Height="450" Width="800">
    <Grid>
		<RichTextBox Name="RTB" IsReadOnly="true" FontFamily="Consolas" Background="Black" Foreground="Lime">
			<FlowDocument>
				<Paragraph></Paragraph>
			</FlowDocument>
		</RichTextBox>
    </Grid>
</Window>
