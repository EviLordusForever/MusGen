<Window x:Class="MusGen.View.Windows.AudioRecreationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusGen.View.Windows"
        mc:Ignorable="d"
        Title="AudioRecreationWindow" Height="450" Width="400">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="5*" />
			<RowDefinition/>
		</Grid.RowDefinitions>

		<Button Width="350" Height="50" Content="Select .wav file" FontSize="25"></Button>
		<Label Content="GG" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1"></Label>
		<Button Width="350" Height="50" Content="Process" FontSize="25" Grid.Row="2"></Button>
	</Grid>	
</Window>
