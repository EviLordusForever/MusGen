<UserControl x:Class="MusGen.ARControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MusGen"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<Grid Height="200" Width="390">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition/>
			<RowDefinition/>
			<RowDefinition/>
		</Grid.RowDefinitions>

		<Button Margin="0,3,0,3" Name ="Btn1" Content="Wav to nad, nad to wav" FontSize="22" Click="Btn1_Click" Grid.Row="0"></Button>
		<Button Margin="0,3,0,3" Name ="Btn2" Content="Wav to jpg" FontSize="22" Click="Btn2_Click" Grid.Row="1"></Button>
		<Button Margin="0,3,0,3" Name ="Btn3" Content="Jpg to wav" FontSize="22" Click="Btn3_Click" Grid.Row="2"></Button>
		<Button Margin="0,3,0,3" Name ="Back" Content="Back" FontSize="22" Click="Back_Click" Grid.Row="3"></Button>
	</Grid>
</UserControl>
